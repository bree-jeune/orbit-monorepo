{
  "manifest_version": 3,
  "name": "Orbit - A Living Space for Attention",
  "short_name": "Orbit",
  "version": "1.0.0",
  "description": "An easier way to remember what matters right now. ",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png",
    "512": "icons/512.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "action": {
    "default_title": "Orbit"
  }
}